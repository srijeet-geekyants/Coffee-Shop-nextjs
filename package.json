{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm next-openapi-gen generate && next dev --turbopack",
    "build": "next build",
    "analyze": "ANALYZE=true pnpm build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type:generate": "next typegen",
    "type:check": "tsc --noEmit",
    "type:circular": "madge src --extensions ts,tsx --circular",
    "prepare": "husky",
    "format": "prettier --write .",
    "next-openapi-gen": "next-openapi-gen",
    "husky:init": "husky init",
    "setup": "node scripts/setup-env.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run src/test/integration",
    "test:integration:watch": "vitest src/test/integration",
    "test:unit": "vitest run --exclude=src/test/integration/**",
    "test:unit:watch": "vitest --exclude=src/test/integration/**",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pg:gen": "cross-env DB_DIALECT=postgresql drizzle-kit generate",
    "db:pg:mig": "cross-env DB_DIALECT=postgresql drizzle-kit migrate",
    "db:mysql:gen": "cross-env DB_DIALECT=mysql drizzle-kit generate",
    "db:mysql:mig": "cross-env DB_DIALECT=mysql drizzle-kit migrate",
    "db:sqlite:gen": "cross-env DB_DIALECT=sqlite drizzle-kit generate",
    "db:sqlite:mig": "cross-env DB_DIALECT=sqlite drizzle-kit migrate",
    "db:pg:studio": "cross-env DB_DIALECT=postgresql drizzle-kit studio",
    "db:mysql:studio": "cross-env DB_DIALECT=mysql drizzle-kit studio",
    "db:sqlite:studio": "cross-env DB_DIALECT=sqlite drizzle-kit studio",
    "db:pg:push": "cross-env DB_DIALECT=postgresql drizzle-kit push",
    "db:mysql:push": "cross-env DB_DIALECT=mysql drizzle-kit push",
    "db:sqlite:push": "cross-env DB_DIALECT=sqlite drizzle-kit push",
    "db:pg:pull": "cross-env DB_DIALECT=postgresql drizzle-kit pull",
    "db:mysql:pull": "cross-env DB_DIALECT=mysql drizzle-kit pull",
    "db:sqlite:pull": "cross-env DB_DIALECT=sqlite drizzle-kit pull",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
